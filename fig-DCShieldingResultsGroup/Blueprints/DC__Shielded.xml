<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.5//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_5.dtd'>
<nta>
	<declaration>/**
I am now certain that this model is associated with the unpublished paper 

Proving and Improving Model Predictive Control of DC-DC Boost Converters
Tomislav Dragičević1 , Peter G. Jensen , Kim G. Larsen , Adrien Le Coënt , Mateja Novak , and Ulrik Nyman

See also source number 7 in that paper:
7. Karamanakos, P., Geyer, T., Manias, S.: Direct voltage control of dc–dc boost converters using enumeration-based model predictive control. IEEE Transactions on Power Electronics 29(2), 968–978 (2014)
*/


// Place global declarations here.
clock x_R = 73.0; // system load
broadcast chan tick;
urgent broadcast chan urg;
clock time;
clock x1 = 0.35; // Current
clock x2 = 15.0; // Voltage


const double x_L = 450e-6;  // capacitor
const double x_RL = 0.3;   
const double x_Co = 220e-6; 
const double x_vs = 10.0;  // input voltage

const double x1ref = 0.35;    // Target values
const double x2ref = 15.0;    // Optimize to stay as close as possible

const double scale = 1.0/0.00005;</declaration>
	<template>
		<name x="5" y="5">Converter</name>
		<declaration>// Place local declarations here.


double A1[2][2];
double A2[2][2];
double A3[2][2];

double B1[2];
double B2[2];
double B3[2];</declaration>
		<location id="id0" x="0" y="0">
			<name x="-51" y="-17">d1u1</name>
			<label kind="invariant" x="-93" y="-102">x1'==(((-x_RL/x_L)*x1)+(x_vs/x_L))/scale&amp;&amp;
x2'==(-(1.0/(x_Co*x_R))*x2)/scale</label>
			<label kind="exponentialrate" x="-17" y="8">1</label>
			<label kind="comments" x="-93" y="-144">X'=A2*x+B,
d_aux = 1</label>
		</location>
		<location id="id1" x="229" y="0">
			<name x="212" y="-34">d1u0</name>
			<label kind="invariant" x="280" y="-102">x1'==((-x_RL/x_L)*x1+(-1.0/x_L)*x2+(x_vs/x_L))/scale&amp;&amp;
x2'==((1.0/x_Co)*x1+(-1.0/(x_Co*x_R))*x2)/scale</label>
			<label kind="exponentialrate" x="212" y="17">1</label>
			<label kind="comments" x="280" y="-144">X'=A1*x+B,
d_aux = 1</label>
		</location>
		<location id="id2" x="229" y="153">
			<name x="212" y="161">d0u0</name>
			<label kind="invariant" x="255" y="136">x1'==0&amp;&amp;
x2'==((-1.0/(x_Co*x_R))*x2)/scale</label>
			<label kind="exponentialrate" x="221" y="204">1</label>
			<label kind="comments" x="255" y="204">X'=A2*x+B,
d_aux = 0</label>
		</location>
		<location id="id3" x="127" y="0">
			<name x="119" y="17">pd1u1</name>
			<committed/>
		</location>
		<location id="id4" x="340" y="0">
			<name x="357" y="-8">pd1u0</name>
			<committed/>
		</location>
		<location id="id5" x="0" y="153">
			<name x="-68" y="144">pu0d0</name>
			<committed/>
		</location>
		<init ref="id0"/>
		<transition id="id6" controllable="false">
			<source ref="id1"/>
			<target ref="id4"/>
			<label kind="synchronisation" x="272" y="25">tick?</label>
			<nail x="289" y="25"/>
		</transition>
		<transition id="id7">
			<source ref="id4"/>
			<target ref="id1"/>
		</transition>
		<transition id="id8">
			<source ref="id5"/>
			<target ref="id2"/>
			<nail x="119" y="178"/>
		</transition>
		<transition id="id9" controllable="false">
			<source ref="id2"/>
			<target ref="id5"/>
			<label kind="synchronisation" x="93" y="127">tick?</label>
		</transition>
		<transition id="id10">
			<source ref="id3"/>
			<target ref="id0"/>
			<nail x="59" y="34"/>
		</transition>
		<transition id="id11" controllable="false">
			<source ref="id0"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="42" y="-17">tick?</label>
		</transition>
		<transition id="id12">
			<source ref="id4"/>
			<target ref="id0"/>
			<nail x="340" y="-42"/>
			<nail x="0" y="-42"/>
		</transition>
		<transition id="id13">
			<source ref="id5"/>
			<target ref="id0"/>
		</transition>
		<transition id="id14" controllable="false">
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="guard" x="238" y="59">x1&lt;=0</label>
			<label kind="synchronisation" x="238" y="76">urg!</label>
		</transition>
		<transition id="id15">
			<source ref="id3"/>
			<target ref="id1"/>
		</transition>
	</template>
	<template>
		<name>Monitor</name>
		<declaration>
clock dist;</declaration>
		<location id="id16" x="-459" y="-34">
			<label kind="invariant" x="-469" y="-17">dist'==pow(x2-x2ref, 2.0)+fabs(x1-x1ref)/3.0</label>
		</location>
		<init ref="id16"/>
	</template>
	<template>
		<name>Timer</name>
		<declaration>clock x;</declaration>
		<location id="id17" x="0" y="0">
			<label kind="invariant" x="-10" y="17">x&lt;=1</label>
		</location>
		<init ref="id17"/>
		<transition id="id18" controllable="false">
			<source ref="id17"/>
			<target ref="id17"/>
			<label kind="guard" x="-33" y="-119">x&gt;=1</label>
			<label kind="synchronisation" x="-33" y="-102">tick!</label>
			<label kind="assignment" x="-33" y="-85">x=0</label>
			<nail x="25" y="-85"/>
			<nail x="-51" y="-85"/>
		</transition>
	</template>
	<template>
		<name>Load</name>
		<location id="id19" x="0" y="0">
			<label kind="invariant" x="-10" y="17">x_R'==0</label>
		</location>
		<location id="id20" x="-76" y="0">
			<committed/>
		</location>
		<location id="id21" x="119" y="-17">
			<committed/>
		</location>
		<init ref="id20"/>
		<transition id="id22" controllable="false">
			<source ref="id21"/>
			<target ref="id19"/>
			<label kind="select" x="76" y="17">i:int[-10,10]</label>
			<label kind="guard" x="76" y="51">x_R + i &gt;= 60 &amp;&amp; x_R + i &lt;= 80</label>
			<label kind="assignment" x="76" y="34">x_R= x_R + i</label>
			<nail x="127" y="17"/>
		</transition>
		<transition id="id23" controllable="false">
			<source ref="id19"/>
			<target ref="id21"/>
			<label kind="synchronisation" x="42" y="-34">tick?</label>
		</transition>
		<transition id="id24" controllable="false">
			<source ref="id20"/>
			<target ref="id19"/>
			<label kind="assignment" x="-76" y="-51">x_R=70.0</label>
		</transition>
	</template>
	<system>
system Converter, Load, Timer, Monitor;
    </system>
	<queries>
		<query>
			<formula/>
			<comment/>
		</query>
		<query>
			<formula>strategy S = minE(Monitor.dist+0.0) [&lt;=1000] {Converter.location} -&gt; {x1, x2}: &lt;&gt; time &gt;= 1000 || (x1 &gt;= 0 &amp;&amp; x1 &lt;= 0.7 &amp;&amp; x2 &gt;= 14.8 &amp;&amp; x2&lt;=15.2) </formula>
			<comment>--print-strategies=strats --discount 0.9 --good-runs 1000 --total-runs 1000 --runs-pr-state 1000 --max-iterations 10 --reset-no-better 10 --random-search-limit 0.5 --deterministic-search-limit 0.8 --split-filter-rate 1 --split-ks 0.2 --split-lower-t 0.05 --split-upper-t 1.8 --learning-method 5 -D0.5</comment>
		</query>
		<query>
			<formula>simulate [&lt;=120;20] {x1,x2, 7 + Converter.d1u1, 9 + Converter.d1u0, 11 + Converter.d0u0, fabs(x2-x2ref), 14.8,  15.2 , 0.7} under S</formula>
			<comment/>
		</query>
		<query>
			<formula>simulate [&lt;=120;10] {x1, x2, 14.8,  15.2 , 0.7}</formula>
			<comment/>
		</query>
		<query>
			<formula>simulate [&lt;=120;10] {x1, x2, 14.8,  15.2 , 0.7} under S</formula>
			<comment/>
		</query>
		<query>
			<formula>E[&lt;=120;100] (max: Monitor.dist)</formula>
			<comment/>
		</query>
		<query>
			<formula>E[&lt;=120;100] (max: Monitor.dist) under S</formula>
			<comment/>
		</query>
		<query>
			<formula>simulate [&lt;=120;2] {x_R}</formula>
			<comment/>
		</query>
		<query>
			<formula>Pr[&lt;=50] (&lt;&gt; (x1 &gt;= 0 &amp;&amp; x1 &lt;= 0.7 &amp;&amp; x2 &gt;= 14.8 &amp;&amp; x2&lt;=15.2) ) under S</formula>
			<comment/>
		</query>
		<query>
			<formula>E[&lt;=50;100] (max:(1-(x1+0.0 &gt;= 0 &amp;&amp; x1+0.0 &lt;= 0.7 &amp;&amp; x2+0.0 &gt;= 14.8 &amp;&amp; x2+0.0&lt;=15.2))*time) under S</formula>
			<comment/>
		</query>
		<query>
			<formula>E[&lt;=50;1000] (max:Monitor.dist*(x1+0.0 &gt;= 0 &amp;&amp; x1+0.0 &lt;= 0.7 &amp;&amp; x2+0.0 &gt;= 14.8 &amp;&amp; x2+0.0&lt;=15.2))  under S</formula>
			<comment/>
		</query>
		<query>
			<formula>E[&lt;=50;1000] (max:Monitor.dist*(x1+0.0 &gt;= 0 &amp;&amp; x1+0.0 &lt;= 0.7 &amp;&amp; x2+0.0 &gt;= 14.8 &amp;&amp; x2+0.0&lt;=15.2))  under S</formula>
			<comment/>
		</query>
		<query>
			<formula>saveStrategy("Results/Misc/DC.strategy.json", S)</formula>
			<comment/>
		</query>
		<query>
			<formula>strategy S = loadStrategy {Converter.location} -&gt; {x1, x2} ("Results/Misc/DC.strategy.json")</formula>
			<comment/>
		</query>
	</queries>
</nta>
