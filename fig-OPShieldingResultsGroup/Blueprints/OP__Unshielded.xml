			<label kind="synchronisation" x="25" y="-127">off?</label>
			<label kind="assignment" x="25" y="-93">l = 2,
p = 0,
switches++</label>
			<nail x="135" y="-8"/>
			<nail x="127" y="-34"/>
			<nail x="17" y="-34"/>
			<nail x="8" y="-8"/>
		</transition>
	</template>
